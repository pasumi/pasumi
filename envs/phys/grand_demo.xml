  <obstacle type="static" name="o2">
    <vertices>
      <vertex>-2.0,-1.0</vertex>
      <vertex>2.0,-1.0</vertex>
      <vertex>2.0,1.0</vertex>
      <vertex>-2.0,1.0</vertex>
    </vertices>
  </obstacle>

  <obstacle type="static" name="o3">
    <vertices>
      <vertex>-2.0,4.0</vertex>
      <vertex>2.0,4.0</vertex>
      <vertex>2.0,5.0</vertex>
      <vertex>-2.0,5.0</vertex>
