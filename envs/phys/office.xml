    <vertices>
      <vertex>-4.0,-4.0</vertex>
      <vertex>-1.0,-4.0</vertex>
      <vertex>-1.0,-1.0</vertex>
      <vertex>-4.0,-1.0</vertex>
    </vertices>
  </obstacle>

  <obstacle type="static" name="o2">
    <vertices>
      <vertex>1.0,-4.0</vertex>
      <vertex>4.0,-4.0</vertex>
      <vertex>4.0,-1.0</vertex>
      <vertex>1.0,-1.0</vertex>
    </vertices>
  </obstacle>

  <obstacle type="static" name="o3">
    <vertices>
      <vertex>1.0,1.0</vertex>
      <vertex>4.0,1.0</vertex>
      <vertex>4.0,4.0</vertex>
      <vertex>1.0,4.0</vertex>
    </vertices>
  </obstacle>

  <obstacle type="static" name="o4">
    <vertices>
      <vertex>-4.0,1.0</vertex>
      <vertex>-1.0,1.0</vertex>
      <vertex>-1.0,4.0</vertex>
