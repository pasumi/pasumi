<env type="physical" name="Office (Phys)">
  <!-- Border -->
  <border name="border">
    <vertices>
      <vertex>6.0,6.0</vertex>
      <vertex>-6.0,6.0</vertex>
      <vertex>-6.0,-6.0</vertex>
      <vertex>6.0,-6.0</vertex>
    </vertices>
    <vertex type="center">0.0,0.0</vertex>
